	</s:complexType>
	<s:complexType name="ArrayOfLink">
		<s:sequence>
			<s:element minOccurs="0" maxOccurs="unbounded" name="Link" nillable="true" type="tns:Link" />
		</s:sequence>
	</s:complexType>
	<s:complexType name="Link">
		<s:sequence>
			<s:element minOccurs="1" maxOccurs="1" name="ID" type="s:int" />
