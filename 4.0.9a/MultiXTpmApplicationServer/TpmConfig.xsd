				<s:complexType mixed="true">
					<s:sequence>
						<s:any maxOccurs="unbounded" />
					</s:sequence>
					<s:anyAttribute />
				</s:complexType>
			</s:element>
		</s:sequence>
	</s:complexType>
	<s:complexType name="ArrayOfInt">
		<s:sequence>
			<s:element minOccurs="0" maxOccurs="unbounded" name="ID" type="s:int" />
