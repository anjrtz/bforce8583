			<s:enumeration value="MultiXLinkTypeLast" />
		</s:restriction>
	</s:simpleType>
	<s:simpleType name="MultiXOpenMode">
		<s:restriction base="s:string">
			<s:enumeration value="MultiXOpenModeFirst" />
